#!/usr/bin/awk -f

BEGIN {
        FS = " ";
        RS = "\n";
        SUBSEP = "\034";
        split("", _Mirrors);
        split("", _Vcs);

        _Vcs["git"]++;

        _Mirrors[ \
                "- name:            DEFAULT",
                "  git_mirror:      true" \
        ]++
}

